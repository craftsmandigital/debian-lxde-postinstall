

        
        <!-- ********************* START INSERTED BY INSTALL SCRIPT ******************* -->
        <!-- All inserted stuff is added to the end of <keyboard> session -->


        <keybind key='W-Left'>
            <action name='Unmaximize'/>
            <action name='MoveResizeTo'>
                <x>0</x>
                <y>0</y>
                <width>50%</width>
                <height>100%</height>
            </action>
        </keybind>
        <keybind key='W-Right'>
            <action name='Unmaximize'/>
            <action name='MoveResizeTo'>
                <x>-0</x>
                <y>0</y>
                <width>50%</width>
                <height>100%</height>
            </action>
        </keybind>
        <keybind key='W-Up'>
            <action name='Unmaximize'/>
            <action name='MoveResizeTo'>
                <x>0</x>
                <y>0</y>
                <width>100%</width>
                <height>50%</height>
            </action>
        </keybind>
        <keybind key='W-Down'>
            <action name='Unmaximize'/>
            <action name='MoveResizeTo'>
                <x>0</x>
                <y>-0</y>
                <width>100%</width>
                <height>50%</height>
            </action>
        </keybind>
        <keybind key='W-g'>
            <action name='Execute'>
                <command>google-chrome-stable --profile-directory=Default</command>
            </action>
        </keybind>
        <keybind key='W-j'>
            <action name='Execute'>
                <command>google-chrome-stable --profile-directory=Jobb</command>
            </action>
        </keybind>
        <keybind key='W-t'>
            <action name='Execute'>
                <command>lxterminal</command>
            </action>
        </keybind>
        <keybind key='W-n'>
            <action name='Execute'>
                <command>leafpad</command>
            </action>
        </keybind>
        <keybind key='W-c'>
            <action name='Execute'>
                <command>code</command>
            </action>
        </keybind>
        <keybind key='W-x'>
            <action name='Execute'>
                <command>lxde-logout</command>
            </action>
        </keybind>
        <keybind key='W-space'>
            <action name='Execute'>
                <command>sh -c &quot;$HOME/git_projects/installs/debian-lxde-postinstall/separate-script/switch-key-lang.sh&quot;</command>
            </action>
        </keybind>


        <!-- ********************* END INSERTED BY INSTALL SCRIPT ******************* -->



